{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": ["react"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "console": "readonly"
  },
  "env": {
    "browser": false,
    "node": true
  },
  "rules": {
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "react/prop-types": 0
  }
}
